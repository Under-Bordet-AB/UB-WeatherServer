{
  "configurations": [
    {
      "name": "WSL",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/src",
        "${workspaceFolder}/src/libs",
        "${workspaceFolder}/src/utils",
        "${workspaceFolder}/src/w_server",
        "${workspaceFolder}/src/w_server/listeners",
        "${workspaceFolder}/src/w_server/middleware"
      ],
      "defines": [],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "gnu99",
      "cppStandard": "gnu++17",
      "intelliSenseMode": "linux-gcc-x64",
      "browse": {
        "path": [
          "${workspaceFolder}/src",
          "${workspaceFolder}/src/libs",
          "${workspaceFolder}/src/utils",
          "${workspaceFolder}/src/w_server",
          "${workspaceFolder}/src/w_server/listeners",
          "${workspaceFolder}/src/w_server/middleware"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      }
    }
  ],
  "version": 4
}